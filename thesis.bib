@article{fleck2023,
  title = {Inferring and Perturbing Cell Fate Regulomes in Human Brain Organoids},
  author = {Fleck, Jonas Simon and Jansen, Sophie Martina Johanna and Wollny, Damian and Zenk, Fides and Seimiya, Makiko and Jain, Akanksha and Okamoto, Ryoko and Santel, Malgorzata and He, Zhisong and Camp, J. Gray and Treutlein, Barbara},
